{
  "name": "plain-env",
  "version": "0.3.1",
  "description": "Auto generate ts/js file from config file and env",
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "b:y-env1": "node ./dist/index.js build ./examples/config/y-env1.yaml=./examples/output/y-env1.ts",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microJ/plain-env.git"
  },
  "keywords": [
    "dotenv",
    "dotenv ts",
    "gen ts env",
    "process.env",
    "env",
    "config",
    "dotenv expand",
    "env-schema",
    "ts-dotenv"
  ],
  "author": "microJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microJ/plain-env/issues"
  },
  "homepage": "https://github.com/microJ/plain-env#readme",
  "files": [
    "dist"
  ],
  "bin": {
    "plain-env": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.6.5",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "yaml": "^2.1.1"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "cac": "^6.7.12",
    "colorette": "^2.0.19",
    "fs-extra": "^10.1.0"
  }
}
